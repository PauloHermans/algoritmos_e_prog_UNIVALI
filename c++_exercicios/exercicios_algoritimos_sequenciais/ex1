#include <iostream>

using namespace std;

int main() {
  float valorPago, preco, troco;
  
  cout << "Digite o Valor do Produto: " << endl;
  cin >> preco;
    
  cout << "Digite o Valor Pago:" << endl;
  cin >> valorPago;
    
  troco = valorPago - preco;

  if (valorPago < preco) {
    cout << "Impossivel Realizar o Pagamento" << endl;
    return 0;
  }
  
  cout << "Receba " << troco << " de Troco" << endl;
  return 0;
}
